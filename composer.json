{
	"name": "lshorz/laravel-captcha",
	"type": "package",
	"description": "Laravel 5 Captcha Package",
	"keywords": ["laravel5.X Captcha", "laravel5 Security", "Captcha"],
	"homepage": "https://github.com/lshorz/laravel-captcha",
	"license": "MIT",
	"authors": [
		{
			"name": "Marven Liao",
			"email": "lshzhuhai@163.com"
		}
	],
	"require": {
		"php": ">=7.0",
		"ext-gd": "*",
		"illuminate/config": "~5.0",
		"illuminate/filesystem": "~5.0",
		"illuminate/support": "~5.0",
		"intervention/image": "~2.2"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.1",
        "mockery/mockery": "0.9.*"
	},
	"autoload": {
		"psr-4": {
			"Lshorz\\LaravelCaptcha\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"minimum-stability": "dev",
	"extra": {
		"laravel": {
			"providers": [
				"Lshorz\\LaravelCaptcha\\CaptchaServiceProvider"
			],
			"aliases": {
				"Captcha": "Lshorz\\LaravelCaptcha\\Facades\\Captcha"
			}
		}
	}
}
